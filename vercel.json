{
  "version": 2,
  "name": "medichain-healthcare-ai",
  "builds": [
    {
      "src": "frontend/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  },
  "functions": {
    "frontend/app.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "regions": [
    "iad1"
  ]
}
